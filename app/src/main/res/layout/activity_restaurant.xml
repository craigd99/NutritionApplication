<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Restaurant">

    <Button
        android:id="@+id/OpenSubway"
        android:layout_width="229dp"
        android:layout_height="105dp"
        android:layout_marginStart="91dp"
        android:layout_marginTop="497dp"
        android:onClick="openSubway"
        android:text="Subway"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/OpenBurgerKing"
        android:layout_width="229dp"
        android:layout_height="105dp"
        android:layout_marginStart="91dp"
        android:layout_marginTop="329dp"
        android:onClick="openBurgerKing"
        android:text="Burger King"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/OpenMcdonalds"
        android:layout_width="229dp"
        android:layout_height="105dp"
        android:layout_marginStart="91dp"
        android:layout_marginTop="174dp"
        android:onClick="openMcdonalds"
        android:text="Mcdonalds"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/OpenKFC"
        android:layout_width="229dp"
        android:layout_height="105dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="44dp"
        android:onClick="openKFC"
        android:text="KFC"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="129dp"
        android:layout_height="25dp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="7dp"
        android:text="Restaurant Options"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>